{
  "name": "polythene-react-setup",
  "version": "1.0.0-rc.8",
  "scripts": {
    "dev": "npm-run-all --parallel dev:watch dev:serve",
    "dev:serve": "webpack-dev-server --config scripts/webpack.config.dev.js --port 3000",
    "dev:watch": "webpack --watch --config scripts/webpack.config.dev.js",
    "build": "webpack -p --config scripts/webpack.config.prod.js"
  },
  "dependencies": {
    "polythene-core": "^1.0.0-rc.8",
    "polythene-css": "^1.0.0-rc.8",
    "polythene-fastclick": "^1.0.0-rc.8",
    "polythene-react": "^1.0.0-rc.8",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "npm-run-all": "^4.1.1",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.3"
  },
  "description": "Polythene for React setup example",
  "license": "MIT"
}
